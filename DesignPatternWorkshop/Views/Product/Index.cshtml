@model IEnumerable<DesignPatternWorkshop.Model.Product>

    @{
    ViewData["Title"] = "Index";
    }

    <h2>Products</h2>

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        
                    </td>
                </tr>
}
        </tbody>
    </table>
